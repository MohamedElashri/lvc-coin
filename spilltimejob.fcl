#include "spilltime.fcl"
#include "services.fcl"

process_name: spilltimejob

services:
{
  message:      @local::standard_out
  @table::standard_services
}

source:
{
  module_type: RootInput
  maxEvents: -1
}

physics:
{
  analyzers:
  {
    spilltime: @local::standard_spilltime
  }

  stream1: [ spilltime ]

  end_paths: [ stream1 ]
}
